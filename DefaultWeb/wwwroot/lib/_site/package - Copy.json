{
  "name": "defaultwebsite.net",
  "description": "Build process for site",
  "version": "1.0.0",
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://hackinc.net",
  "author": "SwartHack",
  "contributors": [
    "The Bootstrap Authors (https://github.com/twbs/bootstrap/graphs/contributors)",
    "Twitter, Inc."
  ],
  "scripts": {
    "clean-css": "cleancss --skip-advanced --source-map --output dist/css/bootstrap.min.css dist/css/bootstrap.css && cleancss --skip-advanced --source-map --output dist/css/bootstrap-grid.min.css dist/css/bootstrap-grid.css && cleancss --skip-advanced --source-map --output dist/css/bootstrap-reboot.min.css dist/css/bootstrap-reboot.css",
    "postcss": "postcss --config grunt/postcss.js --replace dist/css/*/*.css"
  },
  "repository": {},
  "license": "ISC",
  "dependencies": {
    "jquery": ">=1.9.1",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-uglify": "0.11.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "load-grunt-tasks": "^3.5.2",
    "grunt-contrib-sass": "^1.0.0",
    "time-grunt": "^1.4.0"
  },
  "engines": {
    "node": ">=4"
  },
  "sitedist": "_site/dist",
  "bootstrapdir": "/wwwroot/lib/bootstrap",
  "swatchdir": "/wwwroot/lib/bootswatch",
  "cssdist": "_site/dist/css/themes",
  "gruntdir": "_site/grunt",
  "rootsassfile": "sass/SwartlingFamilyWeb.scss",
  "files": [
    "/wwwroot/lib/_site/dist",
    "grunt",
    "/wwwroot/lib/_site/js/**/*.js",
    "/wwwroot/lib/_site/scss/**/*.scss",
    "Gruntfile.js",
    "LICENSE"
  ]
}
